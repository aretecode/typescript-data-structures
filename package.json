{
  "name": "typescript-starter",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "now-build": "tsc || true",
    "build": "tsc --outDir dist || true",
    "start": "node dist/index.js",
    "dev:strict": "STRICT_TYPING=true npx ts-node-dev --n src/index.ts",
    "dev:watch": "NODE_ENV=development ts-node-dev --respawn --transpileOnly src/index.ts",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "test:watch": "jest --config jest.config.js --watch"
  },
  "resolutions": {
    "node-fetch": "2.6.0"
  },
  "dependencies": {
    "express": "4.17.1",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "@types/jest": "24.0.13",
    "typescript": "3.5.1",
    "@types/express": "4.16.1",
    "danger": "7.1.4",
    "prettier": "1.17.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-clean-code": "0.2.9",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tslint-plugin-prettier": "2.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "typescript-eslint-parser": "^22.0.0"
  },
  "license": "MIT",
  "description": "TypeScript Starter",
  "homepage": "https://github.com:aretecode/typescript-starter.git",
  "author": "James Wiens <james@jameswiens.dev> (https://jameswiens.dev)",
  "repository": {
    "type": "git",
    "url": "git@github.com:aretecode/typescript-starter.git"
  },
  "keywords": [
    "jameswiens",
    "james",
    "wiens",
    "dev",
    "aretecode",
    "typescript",
    "starter",
    "js",
    "boilerplate"
  ]
}
